/log/*
/tmp

/public/system/*
/public/storage/development/*
/public/storage/test/*
/public/assets/*
/public/production/*
/public/*.gz
/public/*.css
/public/*.js
/public/*.jpg
/public/*.ico
./*.swp

# Ignore some configs
/config/aws.yml
/config/asset_sync.yml
/config/initializers/smtp_config.rb
/config/database.yml
/config/gdexpress.yml
